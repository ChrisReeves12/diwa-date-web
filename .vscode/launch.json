{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach NextJS Server Side",
      "port": 9231,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "tsx",
      "name": "Run CLI Command",
      "program": "${workspaceFolder}/src/console/cli.ts",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "args": [
        "users:review"
      ]
    }
  ]
}