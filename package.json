{
  "name": "diwa-date-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register --project tsconfig.server.json server.ts",
    "dev:turbo": "next dev --turbopack",
    "dev-debug": "NODE_OPTIONS='--inspect' ts-node -r tsconfig-paths/register --project tsconfig.server.json server.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test --workers=1",
    "test:e2e:parallel": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "cli": "ts-node -r tsconfig-paths/register -P tsconfig.cli.json src/console/cli.ts",
    "cli:build": "tsc -p tsconfig.cli.json && tsc-alias -p tsconfig.cli.json"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.8",
    "@prisma/client": "^6.5.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/uuid": "^10.0.0",
    "amqplib": "^0.10.8",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "15.3.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.0",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-line-awesome": "^1.4.1",
    "sass": "^1.86.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.53.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/amqplib": "^0.10.7",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/pg": "^8.15.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "tailwindcss": "^4",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}